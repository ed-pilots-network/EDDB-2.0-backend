<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet id="21933dfa-df18-4f44-8e6a-0e12d1a419b6" author="daniel-j-mason">
        <createTable tableName="station_system">
            <column name="station_id" type="uuid">
                <constraints primaryKey="true" foreignKeyName="fk_system_station_station_id" referencedTableName="station" referencedColumnNames="id"/>
            </column>
            <column name="system_id" type="uuid">
                <constraints nullable="false" foreignKeyName="fk_system_stations_system_id" referencedTableName="system" referencedColumnNames="id"/>
            </column>
        </createTable>
    </changeSet>

</databaseChangeLog>
