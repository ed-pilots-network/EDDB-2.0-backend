<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet id="46671140-e9da-4601-b356-a854ec4225ff" author="daniel-j-mason">
        <createTable tableName="station_prohibited_commodity">
            <column name="station_id" type="uuid">
                <constraints nullable="false" foreignKeyName="fk_station_prohibited_commodities_station_id" referencedTableName="station" referencedColumnNames="id"/>
            </column>
            <column name="commodity_id" type="uuid">
                <constraints nullable="false" foreignKeyName="fk_station_prohibited_commodities_commodity_id" referencedTableName="commodity" referencedColumnNames="id"/>
            </column>
        </createTable>
        <addPrimaryKey tableName="station_prohibited_commodity" columnNames="station_id, commodity_id" constraintName="pk_station_prohibited_commodity" />
    </changeSet>

</databaseChangeLog>
