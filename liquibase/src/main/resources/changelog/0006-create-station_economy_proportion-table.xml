<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet id="320f5745-af9e-41d2-98f9-4cd023e69064" author="daniel-j-mason">
        <createTable tableName="station_economy_proportion">
            <column name="station_id" type="uuid">
                <constraints nullable="false" foreignKeyName="fk_station_economy_proportion_station_id" referencedTableName="station" referencedColumnNames="id"/>
            </column>
            <column name="economy_id" type="uuid">
                <constraints nullable="false" foreignKeyName="fk_station_economy_proportion_economy_id" referencedTableName="economy" referencedColumnNames="id"/>
            </column>
            <column name="proportion" type="double">
                <constraints nullable="false"/>
            </column>
        </createTable>
        <addPrimaryKey tableName="station_economy_proportion" columnNames="station_id, economy_id" constraintName="pk_station_economy_proportion" />
    </changeSet>

</databaseChangeLog>
